<?xml version="1.0" encoding="utf-8" standalone="no"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:versionCode="197000073" android:versionName="1.97.00.0073">

    <uses-sdk android:minSdkVersion="16" android:targetSdkVersion="31"/>
<!--    <uses-permission android:name="com.symbol.emdk.permission.EMDK" />-->
    <application
        android:allowBackup="true"
        android:name=".Global"
        android:icon="@mipmap/d1sw_logo_foreground"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
<!--        <uses-library android:name="com.symbol.emdk" />-->
        <activity android:exported="true" android:label="DeltaOne Software" android:name="com.honeywell.barcodeexample.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.HOME"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:exported="true" android:label="DeltaOne Software" android:name="com.honeywell.barcodeexample.ScannerActivity">
            <intent-filter>
                <action android:name="android.intent.action.SCANSCREEN"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:exported="true" android:label="DeltaOne Software" android:name="com.honeywell.barcodeexample.ClientBarcodeActivity">
            <intent-filter>
                <action android:name="android.intent.action.CLIENTBARCODEACTIVITY"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:exported="true" android:label="DeltaOne Software" android:name="com.honeywell.barcodeexample.ScannerSelectionBarcodeActivity">
            <intent-filter>
                <action android:name="android.intent.action.SCANNERSELECTBARCODEACTIVITY"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:exported="true" android:label="DeltaOne Software" android:name="com.honeywell.barcodeexample.AnalysisActivity">
            <intent-filter>
                <action android:name="android.intent.action.ANALYSIS"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:exported="true" android:label="DeltaOne Software" android:name="com.honeywell.barcodeexample.SettingsActivity">
            <intent-filter>
                <action android:name="android.intent.action.SETTINGS"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <receiver
            android:name="Global"
            android:exported="true">
            <intent-filter>
                <action
                    android:name="android.intent.action.SETTINGSSET">
                </action>
            </intent-filter>
        </receiver>
    </application>
</manifest>
